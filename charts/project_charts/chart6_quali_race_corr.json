{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
      "text": "Formula 1 Qualifying Position vs Race Outcome",
      "subtitleFontStyle":"italic",
      
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"
    },
  "data": {"url": "https://raw.githubusercontent.com/tahaadamaly/tahaadamaly.github.io/main/python/F1Project/qualirace.csv"},
  "width": "container",
  "height": 310,

  "mark": "rect",
  "encoding": {
    "x": {
      "bin": {"maxbins": 25},
      "field": "qualipos",
      "type": "quantitative",
      "title": "Qualifying Position",
      "axis": {"format": ""}
    },
    "y": {
      "bin": {"maxbins": 25},
      "field": "racepos",
      "type": "quantitative",
      "title": "Final Race Position"
    },
    "color": {
      "aggregate": "count",
      "type": "quantitative",
      "scale": {"scheme": "turbo"}
    }
  },

  "config": {
    "view": {
      "stroke": "transparent"
    }
  }
}
